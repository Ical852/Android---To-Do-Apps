<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplikasirumahsakit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JadwalTugas">
        <activity android:name=".AboutActivity"></activity>
        <activity
            android:name="com.mercubuana.aplikasirumahsakit.SplashActivity"
            android:theme="@style/Theme.JadwalTugas.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mercubuana.aplikasirumahsakit.RincianDataPasienActivity" />
        <activity android:name="com.mercubuana.aplikasirumahsakit.RincianDataKamarActivity" />
        <activity
            android:name="com.mercubuana.aplikasirumahsakit.MainActivity"
            android:theme="@style/Theme.JadwalTugas.NoActionBar"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>